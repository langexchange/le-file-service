<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="nuget" value="https://api.nuget.org/v3/index.json" />
        <add key="github" value="https://nuget.pkg.github.com/langexchange/index.json" />
    </packageSources>
	<packageSourceCredentials>
		<github>
			<add key="Username" value="%NUGET_GITHUB_USERNAME%" />
			<add key="ClearTextPassword" value="%NUGET_GITHUB_PASSWORD%" />
		</github>
	</packageSourceCredentials>
</configuration>