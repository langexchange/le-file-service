{
  "Consul": {
    "Enabled": true,
    "PingEnabled": true,
    "PingEndpoint": "ping",
    "PingInterval": 30,
    "RemoveAfterInterval": 61,
    "RequestRetries": 3,
    "Service": "file-service",
    "Address": "http://le-file-service:80",
    //"Port": "5000"
    "Port": "80"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "Domains": "https://localhost:8500,https://localhost:8500"
  }
}
